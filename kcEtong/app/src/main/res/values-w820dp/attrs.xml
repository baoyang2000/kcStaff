        <attr name="right_visibility" format="string" />
        <attr name="left_bg" format="reference" />
        <attr name="right_bg" format="reference" />
    </declare-styleable>
    <declare-styleable name="IconTitleView">
        <attr name="icenter_text" format="string" />
        <attr name="icenter_text_size" format="dimension" />
        <attr name="icenter_text_color" format="color" />
        <attr name="ibg_color" format="color" />
        <attr name="ileft_visibility" format="string" />
        <attr name="iright_visibility" format="string" />
        <attr name="ileft_src" format="reference" />
        <attr name="iright_src" format="reference" />
        <attr name="inear_right_src" format="reference" />
    </declare-styleable>
